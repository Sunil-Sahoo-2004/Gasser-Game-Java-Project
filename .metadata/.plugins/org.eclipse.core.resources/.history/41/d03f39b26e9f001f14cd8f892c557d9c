package MyProjectPackage;

import java.util.Scanner;

class Gusser {
	int Gussernum;
	
	public int takeNumberGusser() {
		System.out.println("Gusser Guess a number : ");
		Scanner sc = new Scanner(System.in);
		Gussernum = sc.nextInt();
		return Gussernum;
	}	
}

